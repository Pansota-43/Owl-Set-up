<h2> Set Up</h2>
    <script>
    $(document).ready(function(){
    $(".owl-carousel").owlCarousel({
        items: 1,
        loop: true,
        autoplay: true,
        autoplayTimeout: 3000,
        nav: true,
        dots: true,
        responsive: {
            0: {
                items: 1
            },
            600: {
                items: 3
            },
            1000: {
                items: 5
            }
        } // <-- This closing bracket was missing
    }); // <-- Closing bracket for the owlCarousel initialization
});
    </script>
